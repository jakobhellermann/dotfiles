# Input
input * {
	xkb_layout "de"
	xkb_options "ctrl:nocaps"
	xkb_variant "nodeadkeys"

	repeat_delay 200
	repeat_rate 30
}

input type:touchpad {
	tap enabled
	dwt enabled
	natural_scroll enabled
	middle_emulation enabled
	tap_button_map lrm
}

# Output
output * bg /home/jakob/Pictures/wallpapers/sway fill

set $mod Mod1

include config.d/keybinds/*
include config.d/*

include /etc/sway/config.d/*
