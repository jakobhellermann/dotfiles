{
  "theme": "Andromeda",
  "telemetry": {
    "metrics": false,
    "diagnostics": false,
  },
  "vim_mode": true,
  "vim": {
    "use_system_clipboard": "always",
  },
  "autosave": "on_focus_change",
  "cursor_blink": false,
  "gutter": {
    "code_actions": false,
    "folds": false,
  },
  "auto_update": false,
  "collaboration_panel": {
    "button": false,
  },
  "git": {
    "inline_blame": {
      "enabled": false,
    },
  },
  "languages": {
    "Markdown": {
      "format_on_save": "off",
    },
  },
  "prettier": {
    "printWidth": 200,
  },
}
